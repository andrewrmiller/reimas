{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run lint &&  ../node_modules/typescript/bin/tsc",
    "lint": "../node_modules/eslint/bin/eslint.js src/{common,routers}/**/*.ts && ../node_modules/eslint/bin/eslint.js src/app.ts",
    "api": "npm run build && dotenv -e ../../.env -- node ./bin/www",
    "workers": "cd ../workers && npm start",
    "start": "concurrently \"npm run workers\" \"npm run api\"",
    "up": "node ./bin/www"
  },
  "dependencies": {
    "@picstrata/client": "^1.0.15",
    "amqplib": "^0.5.5",
    "common": "0.1.0",
    "concurrently": "^4.1.2",
    "config": "^3.2.2",
    "cookie-parser": "~1.4.4",
    "debug": "^4.1.1",
    "express": "~4.17.3",
    "hbs": "~4.0.4",
    "http-errors": "^1.7.3",
    "js-yaml": "^3.13.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "storage": "0.1.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/config": "0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.5",
    "@types/http-errors": "^1.6.3",
    "@types/js-yaml": "^3.12.1",
    "@types/morgan": "^1.7.36",
    "@types/multer": "^1.4.7",
    "@types/node": "^12.7.4",
    "dotenv-cli": "^7.2.1",
    "ts-node": "^8.3.0"
  }
}
